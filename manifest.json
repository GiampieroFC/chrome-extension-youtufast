{
    "name": "Page Redder",
    "manifest_version": 3,
    "version": "0.1",
    "description": "Turns the page red when you click the icon",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://www.youtube.com/*"
    ]
}